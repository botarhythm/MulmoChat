{
  "name": "MulmoChat",
  "version": "0.0.0",
  "private": true,
  "description": "MulmoChat - AI Multi-modal Chat Application",
  "main": "start.bat",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:server": "tsx server/index.ts",
    "dev:client": "vite",
    "build": "tsc -p tsconfig.client.json && vite build",
    "preview": "vite preview",
    "start": "tsx server/index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.21.0",
    "@openai/realtime-api": "^0.5.0",
    "express": "^4.19.2",
    "mulmocast": "^0.1.0",
    "openai": "^4.67.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.32.0",
    "postcss": "^8.5.2",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.20"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}